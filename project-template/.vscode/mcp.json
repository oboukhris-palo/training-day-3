{
	"servers": {
		"TalkToFigma": {
			"command": "npx",
			"args": [
				"ai-figma-mcp@latest"
			],
			"type": "stdio"
		},
		"github": {
			"type": "http",
			"url": "https://api.githubcopilot.com/mcp/"
		}
	},
	"inputs": [
		{
			"id": "github_token",
			"type": "promptString",
			"description": "GitHub Personal Access Token",
			"password": true
		}
	]
}